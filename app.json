{
  "expo": {
    "name": "BasuraApp",
    "slug": "basura-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo2.png",
    "splash": {
      "image": "./assets/logo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCZmApOQc5xq5rLmkqiOpGG7sqcs7_Z7L4"
      },
      "bundleIdentifier": "com.whoaomi11.basuraapp"
    },
    "android": {
      "package": "com.whoaomi11.basuraapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo2.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "foregroundService": {
        "notificationTitle": "Tu App está usando ubicación",
        "notificationBody": "Estamos rastreando tu ubicación en segundo plano",
        "notificationColor": "#FF0000"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCZmApOQc5xq5rLmkqiOpGG7sqcs7_Z7L4"
        }
      }
    },
    "web": {
      "favicon": "./assets/logo2.png"
    },
    "extra": {
      "eas": {
        "projectId": "0d64cef6-6eb8-49f8-90d2-70074c495455"
      }
    },
    "plugins": ["expo-router"]
  }
}
